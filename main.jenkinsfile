pipeline {
    agent { label 'cloudagent' }
    parameters {
        string(
            defaultValue: 'Szczecin',
            description: '<b>Podaj swoje miasto by sprawdzić pogodę</b>',
            name: 'MIASTO',
            trim: true
        )
    }
    stages {
        stage('Nasza pogoda') {
            steps {
                echo "Tu bedzie moja pogoda wysietlana ${params.MIASTO}"
            }
        }
    }
}